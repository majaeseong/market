<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jaeseong.market.dao.MenuMapper">
	<select id="getAllMenu" resultType="MenuDTO">
		select * from menu
	</select>
	
	<delete id="del_menu" parameterType="int">
		delete from menu where id=#{id}
	</delete>
	
	
</mapper>